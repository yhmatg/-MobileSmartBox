<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/home_back"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/im_logo"
        android:layout_width="@dimen/dp_114"
        android:layout_height="@dimen/dp_68"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/dp_50"
        android:src="@drawable/logo_icon" />

    <ImageView
        android:id="@+id/iv_title"
        android:layout_width="@dimen/dp_238"
        android:layout_height="@dimen/dp_35"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/dp_11"
        android:src="@drawable/home_title" />

    <ImageView
        android:id="@+id/bt_in_out"
        android:layout_width="@dimen/dp_225"
        android:layout_height="@dimen/dp_66"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/dp_70"
        android:background="@drawable/in_out_back" />

    <ImageView
        android:id="@+id/bt_users"
        android:layout_width="@dimen/dp_225"
        android:layout_height="@dimen/dp_66"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/dp_12"
        android:background="@drawable/users_back" />


</LinearLayout>